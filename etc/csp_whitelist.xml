<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="*.datareporter.eu" type="host">*.datareporter.eu</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="*.datareporter.eu" type="host">*.datareporter.eu</value>
            </values>
        </policy>
        <policy id="style-src-elem">
            <values>
                <value id="datareporter-webcache" type="host">https://webcache.datareporter.eu</value>
                <value id="datareporter-webcache-eu" type="host">https://webcache-eu.datareporter.eu</value>
            </values>
        </policy>
        <policy id="script-src-elem">
            <values>
                <value id="datareporter" type="host">webcache.datareporter.eu</value>
                <value id="datareporter-webcache-eu" type="host">webcache-eu.datareporter.eu</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="datareporter-cache-font" type="host">webcachex-eu.datareporter.eu</value>
            </values>
        </policy>

        <policy id="connect-src">
            <values>
                <value id="datareporter" type="host">*.datareporter.eu</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
